<article class = "member">
			<img class="member-image" src="{{url}}" />
        <p class = "name">{{name}}</p>
        <div class = "member-hobbies">
            <ul class="three-hobbies">
              <li class="hobby"> {{hobby1}} </li>
              <li class="hobby"> {{hobby2}} </li>
              <li class="hobby"> {{hobby3}} </li>
            </ul>
        </div>
        <div class = "member-about">
          <p class = "more-about-member">
            {{about}}
          </p>
        </div>
      </article>